<Window x:Class="kalerm_operation_desk.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:kalerm_operation_desk"
        mc:Ignorable="d"
        Title="操作台" Height="400" Width="600" WindowStartupLocation="CenterScreen">

    <Grid>
        <StackPanel Orientation="Horizontal" Grid.Row="4" HorizontalAlignment="Center"  Grid.ColumnSpan="4" VerticalAlignment="Top">
            <Button Style="{StaticResource ButtonStlye}" Name="btnScanAndTestStandard" Height="22" Margin="0,8,0,8" MinWidth="50" Content="流程扫描" Click="btnScanAndTestStandard_Click"/>
            <Button Style="{StaticResource ButtonStlye}" Name="btnScanAndTestStandardGPT" Height="22" Margin="6,8,0,8" MinWidth="50" Content="安规测试扫描" Click="btnScanAndTestStandardGPT_Click"/>
        </StackPanel>
    </Grid>
</Window>
